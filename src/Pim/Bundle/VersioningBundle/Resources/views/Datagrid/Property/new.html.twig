<ul>
{% for fieldKey, fieldValue in value %}
    {% if fieldKey == 'auditData' %}
        {% if fieldValue.new|length %}
        {% for collKey, collValue in fieldValue.new %}
    <li>
        <strong>{{ collKey }}:</strong> {{ collValue|localize_attribute_option(collKey) }}
    </li>
        {% endfor %}
        {% endif %}
    {% else %}
    <li>
        <strong>{{ fieldKey }}:</strong>
        {% if fieldValue.new.timestamp is defined %}
            {{ fieldValue.new|oro_format_datetime }}
        {% elseif fieldValue.new is iterable %}
            {% for newValue in fieldValue.new %}
                {{ newValue|localize_attribute_option(fieldKey) }}
            {% endfor %}
        {% else %}
            {{ fieldValue.new|localize_attribute_option(fieldKey) }}
        {% endif %}
    </li>
    {% endif %}
{% endfor %}
</ul>
