{
    "name": "oro/data-flow-bundle",
    "type": "symfony-bundle",
    "description": "BAP Flexible Entity Bundle",
    "keywords": ["BAP", "Import", "Export"],
    "homepage": "https://github.com/laboro/DataFlowBundle",
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.1.*",
        "jms/serializer-bundle": "0.12.*@dev"
    },
    "require-dev": {
        "doctrine/dbal": ">=2.2,<2.4-dev",
        "doctrine/orm": ">=2.2.3,<2.4-dev"
    },
    "autoload": {
        "psr-0": { "Oro\\Bundle\\DataFlowBundle": "" }
    },
    "target-dir": "Oro/Bundle/DataFlowBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
