function DateTime(field, params)
{
    var value = field && field.nodeName ? {{ getJsFormElementValue('field') }} : field;

    if (isNotDefined(value)) {
        return true;
    }

    // TODO: use datetime formatter insted of direct moment.js usage https://magecore.atlassian.net/browse/BAP-1931
    var momentFormats = localeSettings.getDateTimeFormats('moment');
    if (moment(String(value), momentFormats.datetime, true).isValid()) {
        return true;
    }

    return getComputeMessage(params.message);
}
