<script type="text/javascript">
    require(['jquery', 'underscore', 'oro/datagrid/column-form-listener'],
    function($, _, ColumnFormListener) {
        $(function() {
            /** @type {oro.datagrid.ColumnFormListener} */
            new ColumnFormListener(_.extend({
                datagridName: {{ datagridView.datagrid.name|json_encode|raw }},
                dataField: 'id'
            }, {{ parameters|json_encode|raw }}));
        });
    });
</script>
