{
    "name": "oro/grid-bundle",
    "type": "symfony-bundle",
    "description": "BAP Grid Bundle",
    "keywords": ["BAP", "UI", "User Interface", "Grid"],
    "homepage": "https://github.com/laboro/GridBundle",
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": ">=2.1",
        "oro/ui-bundle": "dev-master",
        "oro/flexible-entity-bundle": "dev-master",
        "oro/filter-bundle": "dev-master",
        "sonata-project/admin-bundle": "2.1.x-dev",
        "sonata-project/doctrine-orm-admin-bundle": "2.1.x-dev"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/laboro/SonataAdminBundle.git"
        }
    ],
    "autoload": {
        "psr-0": { "Oro\\Bundle\\GridBundle": "" }
    },
    "target-dir": "Oro/Bundle/GridBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
